{
    "name": "@storefront-ui/typography",
    "description": "SFUI typography plugin for Tailwind",
    "version": "2.0.0-rc.5",
    "sideEffects": false,
    "license": "MIT",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.umd.js"
        },
        "./*": "./*"
    },
    "source": "index.ts",
    "main": "dist/index.mjs",
    "unpkg": "dist/index.umd.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "dev-only": "vite build --watch",
        "build:typography": "vite build"
    },
    "devDependencies": {
        "tailwind": "^4.0.0",
        "typescript": "^4.9.5",
        "vite": "^4.1.2",
        "vite-plugin-dts": "^2.0.0-beta.1"
    },
    "peerDependencies": {
        "tailwindcss": ">=3.0.0 || >= 3.0.0-alpha.1"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist"
    ],
    "homepage": "https://docs.storefrontui.io/v2/",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vuestorefront/sfui2.git",
        "directory": "packages/sfui/typography"
    },
    "bugs": {
        "url": "https://github.com/vuestorefront/sfui2/issues"
    }
}
