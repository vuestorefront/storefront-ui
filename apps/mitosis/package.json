{
  "name": "@sfui/mitosis",
  "version": "0.0.0",
  "scripts": {
    "build": "mitosis build && yarn remove-react-js && yarn remove-vue-tsx",
    "dev:legacy": "watch 'yarn run build' ./src ./overrides",
    "remove-react-js": "find ./output/react/src/blocks -name \"*.js\" -type f -delete",
    "remove-vue-tsx": "find ./output/vue/vue3/src/blocks -name \"*.lite.tsx\" -type f -delete"
  },
  "dependencies": {
    "@builder.io/mitosis": "0.0.56-109",
    "@builder.io/mitosis-cli": "0.0.17-100",
    "seedrandom": "^3.0.5",
    "vue-loader": "^16.1.0",
    "vue-template-compiler": "^2.7.8"
  },
  "devDependencies": {
    "@builder.io/eslint-plugin-mitosis": "^0.0.1",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "watch": "^1.0.2"
  }
}
