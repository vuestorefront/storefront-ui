{
  "name": "@storefront-ui/preview-nuxt",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt dev --port=3001",
    "build": "PROD=true nuxt build",
    "build:nuxt": "yarn build",
