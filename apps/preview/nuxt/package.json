    "eslint": "eslint \"**/*.{js,ts,vue}\"",
    "prettier": "prettier --log-level warn --no-error-on-unmatched-pattern \"**/*.{js,ts,vue}\""
  },
  "devDependencies": {
    "@frsource/autoresize-textarea": "^2.0.68",
