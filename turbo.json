{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "cache": false,
      "dependsOn": [
        "^build:shared",
        "^build:typography",
        "build:tailwind-config",
        "build:release",
        "build:next",
        "build:nuxt"
      ],
      "outputs": ["dist/**"]
    },
    "lint": {
      "dependsOn": ["^build:shared"],
      "outputs": []
    },
    "lint:fix": {
      "cache": false,
      "outputs": []
    },
    "lint:fix:sfui": {
      "cache": false,
      "outputs": []
    },
    "build:next": {
      "cache": false,
      "dependsOn": ["^build:react", "^build:typography", "build:tailwind-config"]
    },
    "build:nuxt": {
      "cache": false,
      "dependsOn": ["^build:vue", "^build:typography", "build:tailwind-config"]
    },
    "build:docs": {
      "dependsOn": ["^build:tailwind-config"],
      "cache": false
    },
    "build:react": {
      "dependsOn": ["^build:shared"],
      "cache": false
    },
    "build:vue": {
      "dependsOn": ["^build:shared"],
      "cache": false
    },
    "build:release": {
      "dependsOn": [
        "build:tw-plugin-peer-next",
        "build:react",
        "build:vue",
        "build:tailwind-config",
        "build:nuxt-module",
        "build:typography",
        "build:peer-next",
        "build:shared"
      ],
      "cache": false
    },
    "build:shared": {
      "cache": false
    },
    "build:typography": {
      "cache": false
    },
    "build:nuxt-module": {
      "cache": false
    },
    "build:peer-next": {
      "cache": false
    },
    "build:tailwind-config": {
      "cache": false
    },
    "build:tw-plugin-peer-next": {
      "cache": false
    },
    "build:test-utils": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "dev:docs": {
      "cache": false,
      "dependsOn": ["^build:typography", "build:peer-next", "build:tailwind-config"]
    },
    "dev:shared": {
      "cache": false
    },
    "test": {
      "dependsOn": ["build:test-utils", "dev:shared", "^build:tailwind-config"],
      "cache": false
    },
    "test:ci-pre:vue": {
      "dependsOn": [
        "^build:shared",
        "^build:typography",
        "build:peer-next",
        "build:tailwind-config",
        "build:test-utils"
      ],
      "cache": false
    },
    "test:ci:vue": {
      "dependsOn": [
        "^build:shared",
        "^build:typography",
        "build:peer-next",
        "build:tailwind-config",
        "build:test-utils"
      ],
      "cache": false
    },
    "test:ci-pre:react": {
      "dependsOn": [
        "^build:shared",
        "^build:typography",
        "build:peer-next",
        "build:test-utils",
        "build:tailwind-config"
      ],
      "cache": false
    },
    "test:ci:react": {
      "dependsOn": [
        "^build:shared",
        "^build:typography",
        "build:peer-next",
        "build:test-utils",
        "build:tailwind-config"
      ],
      "cache": false
    }
  }
}
